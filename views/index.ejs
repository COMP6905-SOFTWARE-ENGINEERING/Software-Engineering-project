    $(document).ready(function() {

        demo.initChartist();

        $.notify({
            icon: 'pe-7s-gift',
            message: "Welcome to <b>Graduate Recruitment System </b> dashboard."

        }, {
            type: 'info',
            timer: 4000
        });

    });
</script>


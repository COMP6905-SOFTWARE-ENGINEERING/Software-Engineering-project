<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8" />


    <title>Dashboard</title>

    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <meta name="viewport" content="width=device-width" />

    <!-- Bootstrap core CSS     -->
    <link href="/css/bs.min.css" rel="stylesheet" />

    <!--  Light Bootstrap Table core CSS    -->
    <link href="/css/light-bootstrap-dashboard.css" rel="stylesheet" />
    <!--     Fonts and icons     -->
    <link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">
    <link href='http://fonts.googleapis.com/css?family=Roboto:400,700,300' rel='stylesheet' type='text/css'>
    <link href="/css/dashboardAttach.css" rel="stylesheet" />
</head>

<body>

<div class="wrapper">
    <div class="sidebar side" data-color="black"  data-image="">

        <div class="sidebar-wrapper">
            <div class="logo">
                <img src="https://images.vexels.com/media/users/3/147101/isolated/preview/b4a49d4b864c74bb73de63f080ad7930-instagram-profile-button-by-vexels.png" style="width:150px" border=5>
            </div>

            <ul class="nav">
                <li class="active">
                    <a href="dashboard.ejs">
                        <i class="pe-7s-graph"></i>
                        <p>Dashboard</p>
                    </a>
                </li>
                <li>
                    <a href="/dashboard/profile_student_view"  >
                        <i class="pe-7s-user" onclick="viewProfile()"></i>
                        <p>View Profile</p>
                    </a>
                </li>

                <li>
                    <a href="/match/result">
                        <i class="pe-7s-note2"></i>
                        <p>Matched Projects</p>
                    </a>
                </li>
                <li>
                    <a href="">
                        <i class="pe-7s-note2"></i>
                        <p>Progress</p>
                    </a>
                </li>
                <li>
                    <a href="/user/create_profile">
                        <i class="pe-7s-note2"></i>
                        <p>Create Profile</p>
                    </a>
                </li>



            </ul>
        </div>
    </div>

    <div class="main-panel">
        <nav class="navbar navbar-default navbar-fixed">
            <div class="container-fluid">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navigation-example-2">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href="#">Dashboard</a>

                </div>
                <div class="collapse navbar-collapse">
                    <ul class="nav navbar-nav navbar-left">
                        <li>
                            <a href="">
                                <i class="fa fa-4x fa-user" >&nbsp;</i>
                            </a>
                        </li>
                        <li>
                            <a href="">
                                <i class="fa fa-envelope"></i>
                            </a>
                        </li>
                        <li>
                            <a href="">
                                <i class="fa fa-4x fa-thumbs-up">&nbsp;</i>
                            </a>
                        </li>
                        <li>
                            <a href="">
                                <i class="fa fa-4x fa-book">&nbsp;</i>
                            </a>
                        </li>
                        <li>
                            <a href="">
                                <i class="fa fa-4x fa-bell">&nbsp;</i>
                            </a>
                        </li>

                    </ul>

                    <ul class="nav navbar-nav navbar-right">
                        <li>
                            <a href="#">
                                <span>Mahesh</span>

                            </a>

                        </li>
                        <li>

                            <a href="#">
                                <span>Logout</span>
                                <i class="fa fa-sign-out"></i>
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>

        <div class="content">
            <div class="container">
                <section class="cms-boxes">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-md-4 cms-boxes-outer " >
                                <div class="cms-boxes-items cms-features radChange" id="profile" onclick="viewProfile()" >
                                    <div class="boxes-align">
                                        <div class="small-box">
                                            <i class="fa fa-4x fa-user" onclick="viewProfile()">&nbsp;</i>
                                            <h3>Profile</h3>
                                            <p>Checkout your profile here</p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-1 cms-boxes-outer ">
                            </div>
                            <div class="col-md-4 cms-boxes-outer ">
                                <div class="cms-boxes-items cms-features radChange" id="match" onclick="viewMatches()">
                                    <div class="boxes-align">
                                        <div class="small-box">
                                            <i class="fa fa-4x fa-thumbs-up" onclick="viewMatches()">&nbsp;</i>


                                            <h3>Matched Projects</h3>
                                            <p>You got new match</p>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-4 cms-boxes-outer ">
                                <div class="cms-boxes-items cms-features radChange" id="chat">
                                    <div class="boxes-align">
                                        <div class="small-box">
                                            <i class="fa fa-4x fa-envelope">&nbsp;</i>
                                            <h3>Messenger</h3>
                                            <p>You got a new message</p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-1 cms-boxes-outer ">
                            </div>
                            <div class="col-md-4 cms-boxes-outer ">
                                <div class="cms-boxes-items cms-features radChange" id="grades">
                                    <div class="boxes-align">
                                        <div class="small-box">
                                            <i class="fa fa-4x fa-book">&nbsp;</i>
                                            <h3>Grades</h3>
                                            <p>Check out your performance here</p>
                                        </div>
                                    </div>
                                </div>
                            </div>


                        </div></div>

            </div>
            </section>
        </div>
    </div>
</div>
</div>

</div>
</div>



<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8" />


    <title>Dashboard</title>

    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <meta name="viewport" content="width=device-width" />

    <!-- Bootstrap core CSS     -->
    <link href="/css/bs.min.css" rel="stylesheet" />
    <script src="https://code.jquery.com/jquery-2.1.4.js"></script>
    <!--  Light Bootstrap Table core CSS    -->
    <link href="/css/light-bootstrap-dashboard.css" rel="stylesheet" />
    <!--     Fonts and icons     -->
    <link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">
    <link href='http://fonts.googleapis.com/css?family=Roboto:400,700,300' rel='stylesheet' type='text/css'>
    <link href="/css/dashboardAttach.css" rel="stylesheet" />
    <script type="text/javascript" src="../javascripts/jquery_index.js"></script>

</head>

<body>

<div class="wrapper">
    <div class="sidebar side" data-color="black"  style="background-image:url('../images/dashHome.png')" data-image="">

        <div class="sidebar-wrapper" >
            <div class="logo">
                <img src="https://yt3.ggpht.com/a-/AN66SAwl_LnL7rCVjo6Sb4Yfep6IoBo8eQLy47s-QA=s900-mo-c-c0xffffffff-rj-k-no" style="width:150px" border=5>
            </div>

            <ul class="nav">
                <li class="active">
                    <a href="#">
                        <i class="pe-7s-graph"></i>
                        <p>Dashboard</p>
                    </a>
                </li>
                <% if (userdata.usertype == 'student'){ %>
                    <li>
                        <a href="/dashboard/profile_student_view"  >
                            <i class="pe-7s-user" onclick="viewProfile()"></i>
                            <p>View Profile</p>
                        </a>
                    </li>

                    <li>
                        <a href="match/result_for_student">
                            <i class="pe-7s-note2"></i>
                            <p>Matched Projects</p>
                        </a>
                    </li>
                    <li>
                        <a href="/dashboard/monitor">
                            <i class="pe-7s-note2"></i>
                            <p>Progress</p>
                        </a>
                    </li>
                    <li>
                        <a href="/user/create_profile">
                            <i class="pe-7s-note2"></i>
                            <p>Create Profile</p>
                        </a>
                    </li>
                <% }else if (userdata.usertype == 'manager') { %>
                    <li>
                        <a href="/project/create_project">
                            <i class="pe-7s-note2"></i>
                            <p>Create Project</p>
                        </a>
                    </li>
                    <li>
                        <a href="/project/projectlist_manager"  >
                            <i class="pe-7s-user" onclick="viewProfile()"></i>
                            <p>List of Projects</p>
                        </a>
                    </li>

                    <li>
                        <a href="/match/result_for_manager">
                            <i class="pe-7s-note2"></i>
                            <p>Matched Projects</p>
                        </a>
                    </li>
                    <li>
                        <a href="/dashboard/student_progress">
                            <i class="pe-7s-note2"></i>
                            <p>Students Progress</p>
                        </a>
                    </li>

                <% }else { %>

                <% } %>




            </ul>
        </div>
    </div>

    <div class="main-panel">
        <nav class="navbar navbar-default navbar-fixed">
            <div class="container-fluid">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navigation-example-2">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href="#">Dashboard</a>

                </div>
                <div class="collapse navbar-collapse">
                    <% if (userdata.usertype == 'student'){ %>
                    <ul class="nav navbar-nav navbar-left">
                        <li>
                            <a href="/dashboard/profile_student_view">
                                <i class="fa fa-4x fa-user" >&nbsp;</i>
                            </a>
                        </li>
                        <li>
                            <a href="match/communication">
                                <i class="fa fa-envelope"></i>
                            </a>
                        </li>
                        <li>
                            <a href="match/result_for_student">
                                <i class="fa fa-4x fa-thumbs-up">&nbsp;</i>
                            </a>
                        </li>
                        <li>
                            <a href="/dashboard/monitor">
                                <i class="fa fa-4x fa-book">&nbsp;</i>
                            </a>
                        </li>


                    </ul>
                        <% }else if (userdata.usertype == 'manager') { %>
                            <ul class="nav navbar-nav navbar-left">
                                <li>
                                    <a href="/project/create_project">
                                        <i class="fa fa-4x fa-user" >&nbsp;</i>
                                    </a>
                                </li>
                                <li>
                                    <a href="/match/communication">
                                        <i class="fa fa-envelope"></i>
                                    </a>
                                </li>
                                <li>
                                    <a href="/match/result_for_manager">
                                        <i class="fa fa-4x fa-thumbs-up">&nbsp;</i>
                                    </a>
                                </li>
                                <li>
                                    <a href="/project/projectlist_manager">
                                        <i class="fa fa-4x fa-book">&nbsp;</i>
                                    </a>
                                </li>


                            </ul>
                        <% } %>
                    <ul class="nav navbar-nav navbar-right">
                        <li>
                            <a href="#">
                                <span><%= userdata.firstname %></span>

                            </a>

                        </li>


                        <li class="nav-item">
                            <a class="nav-link logout_area" href="">Logout</a>
                        </li>

                    </ul>
                </div>
            </div>
        </nav>
        <% if (userdata.usertype == 'student'){ %>
            <div class="content">
                <div class="container">
                    <section class="cms-boxes">
                        <div class="container-fluid">
                            <div class="row">
                                <div class="col-md-4 cms-boxes-outer " >
                                    <div class="cms-boxes-items cms-features radChange" id="profile" onclick="viewProfile()" >
                                        <div class="boxes-align">
                                            <div class="small-box">
                                                <i class="fa fa-4x fa-user" onclick="viewProfile()">&nbsp;</i>
                                                <h3>Profile</h3>
                                                <p>Checkout your profile here </p>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-1 cms-boxes-outer ">
                                </div>
                                <div class="col-md-4 cms-boxes-outer ">
                                    <div class="cms-boxes-items cms-features radChange" id="match" onclick="viewMatchesStudent()">
                                        <div class="boxes-align">
                                            <div class="small-box">
                                                <i class="fa fa-4x fa-thumbs-up" onclick="viewMatches()">&nbsp;</i>


                                                <h3>Matched Projects</h3>
                                                <p>Checkout your matched projects here</p>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-md-4 cms-boxes-outer ">
                                    <div class="cms-boxes-items cms-features radChange" id="chat" onclick="chatBox()">
                                        <div class="boxes-align">
                                            <div class="small-box">
                                                <i class="fa fa-4x fa-envelope">&nbsp;</i>
                                                <h3>Messenger</h3>
                                                <p>You can check your messages here</p>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-1 cms-boxes-outer ">
                                </div>
                                <div class="col-md-4 cms-boxes-outer ">
                                    <div class="cms-boxes-items cms-features radChange" id="grades" onclick="monitoring()">
                                        <div class="boxes-align">
                                            <div class="small-box">
                                                <i class="fa fa-4x fa-book">&nbsp;</i>
                                                <h3>My Progress</h3>
                                                <p>Check out your performance here</p>
                                            </div>
                                        </div>
                                    </div>
                                </div>


                            </div>
                        </div>


                    </section>
                </div>
            </div>
        <% }else if (userdata.usertype == 'manager') { %>
            <div class="content">
                <div class="container">
                    <section class="cms-boxes">
                        <div class="container-fluid">
                            <div class="row">
                                <div class="col-md-4 cms-boxes-outer " >
                                    <div class="cms-boxes-items cms-features radChange" id="profile" onclick="viewProfileManager()" >
                                        <div class="boxes-align">
                                            <div class="small-box">
                                                <i class="fa fa-4x fa-user" onclick="viewProfileManager()">&nbsp;</i>
                                                <h3>My Projects</h3>
                                                <p>Checkout all projects here</p>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-1 cms-boxes-outer ">
                                </div>
                                <div class="col-md-4 cms-boxes-outer ">
                                    <div class="cms-boxes-items cms-features radChange" id="match" onclick="viewMatchesProf()">
                                        <div class="boxes-align">
                                            <div class="small-box">
                                                <i class="fa fa-4x fa-thumbs-up" onclick="viewMatches()">&nbsp;</i>


                                                <h3>Matched Projects</h3>
                                                <p>Checkout all matched projects</p>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-md-4 cms-boxes-outer ">
                                    <div class="cms-boxes-items cms-features radChange" id="chat" onclick="chatBox()">
                                        <div class="boxes-align">
                                            <div class="small-box">
                                                <i class="fa fa-4x fa-envelope">&nbsp;</i>
                                                <h3>Messenger</h3>
                                                <p>You can check your messages here</p>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-1 cms-boxes-outer ">
                                </div>
                                <div class="col-md-4 cms-boxes-outer ">
                                    <div class="cms-boxes-items cms-features radChange" id="grades" onclick="studentsProgress()">
                                        <div class="boxes-align">
                                            <div class="small-box">
                                                <i class="fa fa-4x fa-book"  href="student_progress.ejs">&nbsp;</i>
                                                <h3>Students Progress</h3>
                                                <p>Check out your students performance here</p>
                                            </div>
                                        </div>
                                    </div>
                                </div>


                            </div>
                        </div>


                    </section>
                </div>
            </div>
        <% }else { %>

        <% } %>

</div>
</div>

</div>
</div>


</body>

<!--   Core JS Files   -->
<script src="../javascripts/jquery-1.10.2.js" type="text/javascript"></script>
<script src="../javascripts//bootstrap.min.js" type="text/javascript"></script>
<script src="../javascripts/jquery_dashboard.js"></script>
<script src="../javascripts/light-bootstrap-dashboard.js"></script>



<script type="text/javascript">
    $(document).ready(function() {

        $.notify({
            icon: 'pe-7s-gift',
            message: "Welcome to <b>Graduate Recruitment System </b> dashboard."

        }, {
            type: 'info',
            timer: 4000
        });

    });
</script>

</html>